{
    "name": "humans/settings",
    "description": "Class-based settings handler for PHP.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaggy",
            "email": "jaggy@humans.ph"
        }
    ],
    "require": {
        "illuminate/support": "^7.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "symfony/var-dumper": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Humans\\Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Humans\\Settings\\Laravel\\SettingsServiceProvider"
            ]
        }
    }
}
