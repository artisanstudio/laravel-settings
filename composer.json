{
    "name": "artisan/laravel-settings",
    "description": "A settings package for Laravel using property bags.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jaggy",
            "email": "jaggy@artisan.studio"
        }
    ],
    "require": {
        "illuminate/support": "^5.7|^6.0",
        "illuminate/database": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Artisan\\Settings\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Artisan\\Settings\\ServiceProvider"
            ]
        }
    }
}
